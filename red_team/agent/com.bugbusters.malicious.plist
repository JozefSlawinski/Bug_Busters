<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.bugbusters.malicious</string>
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>/Library/Application Support/BugBusters/malicious_agent.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StartInterval</key>
    <integer>300</integer>
    <key>StandardOutPath</key>
    <string>/Library/Application Support/BugBusters/agent.log</string>
    <key>StandardErrorPath</key>
    <string>/Library/Application Support/BugBusters/agent_error.log</string>
    <key>WorkingDirectory</key>
    <string>/Library/Application Support/BugBusters</string>
</dict>
</plist>

